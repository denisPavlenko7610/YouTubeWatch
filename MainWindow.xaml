<Window x:Class="YouTubeRealtimeStats.YouTubeWatch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="YouTube Realtime Stats" Height="242" Width="241" Loaded="Window_Loaded"
        Background="#333333">
    <StackPanel Orientation="Vertical">
        <Label x:Name="lblSubscriberCount" HorizontalAlignment="Left" FontWeight="Bold" FontSize="16" Foreground="White" Margin="10" Width="Auto"/>
        <Label x:Name="lblViewCount" HorizontalAlignment="Left" FontWeight="Bold" FontSize="16" Foreground="White" Margin="10" Width="Auto"/>
        <Label x:Name="lblVideosCount" HorizontalAlignment="Left" FontWeight="Bold" FontSize="16" Foreground="White" Margin="10" Width="Auto"/>
    </StackPanel>
</Window>
